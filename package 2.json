{
  "name": "multi-input-component",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 ios_saf versions",
    "last 2 Safari versions"
  ],
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
    "*.{css,scss}": "stylelint",
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cleanup": "rm yarn.lock; rm package-lock.json; rm -rf node_modules",
    "deploy": "npm run build && firebase deploy",
    "lint:css": "stylelint './src/**/*.jsx'",
    "lint:js": "eslint --fix --ext '.jsx,.js' './src/**/*'",
    "precommit": "lint-staged",
    "reboot": "npm run cleanup; npm install",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-powerplug": "^1.0.0-rc.1",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.3",
    "styled-media-query": "^2.0.2",
    "styled-normalize": "^8.0.0",
    "styled-system": "^3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^3.0.1",
    "stylelint-order": "^0.8.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "@storybook/react": "^3.4.8",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nygdjs/multi-input"
  }
}
